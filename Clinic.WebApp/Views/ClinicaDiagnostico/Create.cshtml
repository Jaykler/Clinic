@model Clinic.WebApp.Models.ClinicaDiagnostico

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>ClinicaDiagnostico</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
           
            <div class="form-group">
                <label asp-for="IdhistoricaClinica" class="control-label"></label>
                <input asp-for="IdhistoricaClinica" class="form-control" />
                <span asp-validation-for="IdhistoricaClinica" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Identificacion" class="control-label"></label>
                <input asp-for="Identificacion" class="form-control" />
                <span asp-validation-for="Identificacion" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Iddoctor" class="control-label"></label>
                <input asp-for="Iddoctor" class="form-control" />
                <span asp-validation-for="Iddoctor" class="text-danger"></span>
            </div>
            
            <div class="form-group">
                <label asp-for="Diagnosticos" class="control-label"></label>
                <input asp-for="Diagnosticos" class="form-control" />
                <span asp-validation-for="Diagnosticos" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Observaciones" class="control-label"></label>
                <input asp-for="Observaciones" class="form-control" />
                <span asp-validation-for="Observaciones" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ExamenLab" class="control-label"></label>
                <input asp-for="ExamenLab" class="form-control" />
                <span asp-validation-for="ExamenLab" class="text-danger"></span>
            </div>
            
            <div class="form-group py-3">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index" class="btn btn-danger">Cancelar</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
