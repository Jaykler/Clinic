@model IEnumerable<Clinic.WebApp.Models.ClinicaDiagnostico>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Iddiagnostico)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdhistoricaClinica)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Identificacion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Iddoctor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaDiagnostico)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Diagnosticos)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Observaciones)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ExamenLab)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Estado)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Iddiagnostico)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdhistoricaClinica)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Identificacion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Iddoctor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaDiagnostico)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Diagnosticos)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Observaciones)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ExamenLab)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Estado)
            </td>
            <td>
                    <a asp-controller="ClinicaDiagnostico" asp-action="Edit" asp-route-id="@item.Iddiagnostico" class="btn btn-success"><i class="bi bi-pencil-square"></i></a>

                    <a asp-controller="ClinicaDiagnostico" asp-action="Delete" asp-route-id="@item.Iddiagnostico" class="btn btn-danger"><i class="bi bi-trash-fill"></i></a>
            </td>
        </tr>
}
    </tbody>
</table>
